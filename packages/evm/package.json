{
  "name": "multiprice-oracle",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v3-core": "^1.0.0",
