    "hardhat": "^2.2.1",
    "hardhat-local-networks-config-plugin": "^0.0.5",
    "inquirer": "^6.5.2",
    "prettier": "^2.3.0",
    "prettier-plugin-solidity": "*",
    "solhint": "^3.3.4",
    "solhint-plugin-prettier": "^0.0.5"
